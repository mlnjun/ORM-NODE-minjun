
            <!--우측 콘텐츠 헤더영역 -->
            <div class="row wrapper border-bottom white-bg page-heading">
                <div class="col-lg-10">
                    <h2>채팅 채널 관리</h2>
                    <ol class="breadcrumb">
                        <li>
                            <a href="/">Home > </a>
                        </li>
                        <li>
                            <a>채팅 채널 관리 > </a>
                        </li>
                        <li class="active">
                            <strong>생성</strong>
                        </li>
                    </ol>
                </div>
                <div class="col-lg-2">

                </div>
            </div>

            <!--우측 콘텐츠 영역 -->
            <form action="/channel/modify/<%=channel.community_id%>" method="post">
                <div class="wrapper wrapper-content">
                    <div class="ibox-content m-b-sm border-bottom">
    
    
                        <div class="row">
                            <div class="col-sm-6">
                                <div class="form-group">
                                    <label class="control-label">제목</label>
                                    <input type="text" class="form-control" value="<%=channel.channel_name%>">
                                </div>
                            </div>
                            
                            </div>
                            <div class="row">
                            <div class="col-sm-4">
                                <div class="form-group">
                                <label class="control-label">작성자</label>
                                <input type="text" class="form-control" value="<%=channel.reg_member_id%>">
                                </div>
                            </div>
                            <div class="col-sm-4">
                                <div class="form-group">
                                    <label class="control-label">카테고리</label>
                                    <select class="form-control">
                                        <option value="0" <% if (channel.category_code == 0) { %> selected <%}%>>선택</option>
                                        <option value="1" <% if (channel.category_code == 1) { %> selected <%}%>>백엔드 채팅방</option>
                                        <option value="2" <% if (channel.category_code == 2) { %> selected <%}%>>프론트엔드 채팅방</option>
                                    </select>
                                </div>
                            </div>
    
                            <div class="col-sm-4">
                                <div class="form-group">
                                    <label class="control-label">인원수</label>
                                    <select class="form-control">
                                        <option value="0" <% if (channel.user_limit == 0) { %> selected <%}%>>0</option>
                                        <option value="1" <% if (channel.user_limit == 1) { %> selected <%}%>>1</option>
                                        <option value="2" <% if (channel.user_limit == 2) { %> selected <%}%>>2</option>
                                        <option value="3" <% if (channel.user_limit == 3) { %> selected <%}%>>3</option>
                                        <option value="4" <% if (channel.user_limit == 4) { %> selected <%}%>>4</option>
                                        <option value="5" <% if (channel.user_limit == 5) { %> selected <%}%>>5</option>
                                        <option value="6" <% if (channel.user_limit == 6) { %> selected <%}%>>6</option>
                                        <option value="7" <% if (channel.user_limit == 7) { %> selected <%}%>>7</option>
                                        <option value="8" <% if (channel.user_limit == 8) { %> selected <%}%>>8</option>
                                        <option value="9" <% if (channel.user_limit == 9) { %> selected <%}%>>9</option>
                                        <option value="10" <% if (channel.user_limit == 10) { %> selected <%}%>>10</option>
                                    </select>
                                </div>
                            </div>
                        </div>
    
                        <div class="row">
    
                            <div class="col-sm-12">
                                <div class="form-group">
                                    <label class="control-label">소개</label>
                                    <textarea class="form-control" rows="5" cols="12"><%=channel.channel_desc%></textarea>
                                </div>
                            </div>
    
                        </div>

                        <div class="row">
    
                            <div class="col-sm-4">
                                <div class="form-group">
                                <label class="control-label">공개/비공개</label>
                                <select class="form-control">
                                    <option value="0" <% if (channel.channel_state_code == 0) { %> selected <%}%>>선택</option>
                                    <option value="1" <% if (channel.channel_state_code == 1) { %> selected <%}%>>공개</option>
                                    <option value="2" <% if (channel.channel_state_code == 2) { %> selected <%}%>>비공개</option>
                                </select>
                                </div>
                            </div>
    
                        </div>
    
    
                        <div class="text-center">
                            <button type="submit" class="btn btn-primary">수정</button>
                            <a href="/channel/delete" class="btn btn-danger">삭제</a>
                            <a href="/channel/list" class="btn btn-info">목록</a>
                        </div>
    
                    </div>
                </div>

            </form>
